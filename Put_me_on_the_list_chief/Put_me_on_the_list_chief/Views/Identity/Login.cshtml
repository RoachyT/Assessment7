@model Put_me_on_the_list_chief.Models.RegistrationModel


<h2>Login</h2>

@using (Html.BeginForm("Login", "Identity", FormMethod.Post))

{
    @Html.ValidationSummary()
    @Html.LabelFor(x => x.Email) @Html.TextBoxFor(x => x.Email)<br />
    @Html.LabelFor(x => x.Password) @Html.PasswordFor(x => x.Password)<br />



    <input class="btn-info" type="submit" value="Login" />

}
